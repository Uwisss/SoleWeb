{
  "version": 2,
  "builds": [
    {
      "src": "SoleWeb/public/index.php",
      "use": "@vercel/php"
    },
    {
      "src": "SoleWeb/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/SoleWeb/public/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/SoleWeb/public/index.php"
    }
  ]
}
